{
    "version": 2,
    "functions": {
      "api/index.js": {
        "runtime": "nodejs18.x",
        "memory": 256,
        "maxDuration": 10
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "src": "/(.*)", "status": 404, "dest": "/api/index.js" }
    ]
  }